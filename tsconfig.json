{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"strict": true,
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "dist",
		"sourceMap": false,
		"baseUrl": ".",
		"paths": { "@/*": ["src/*"] },
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
